{
  "name": "i18n-downloader",
  "displayName": "i18n",
  "description": "i18n downloader",
  "repository": "git@github.com:beMySun/i18n.git",
  "publisher": "scaukk",
  "version": "0.0.5",
  "engines": {
    "vscode": "1.46.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/i18n.png",
  "activationEvents": [
    "onCommand:i18n.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "i18n.helloWorld",
        "title": "Hello World!"
      },
      {
        "command": "i18n.downloadI18nResources",
        "title": "download i18n resources"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "i18n.helloWorld",
          "group": "navigation"
        },
        {
          "when": "editorFocus",
          "command": "i18n.downloadI18nResources",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "1.45.1",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "isomorphic-fetch": "^2.2.1",
    "path": "^0.12.7"
  }
}
